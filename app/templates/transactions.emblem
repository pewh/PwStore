sub-header
	form.navbar-form.navbar-left role="search"
		div.form-group
			= input class='form-control' type='search' valueBinding='filter' placeholder='Pencarian'

	div.nav.navbar-nav.navbar-right
		bs-button clicked='showCreateForm' type='primary' icon='glyphicon glyphicon-plus' class='navbar-btn' title='Tambah Transaksi'

.row
	.col-sm-8
		table.table
			thead: tr
				th Tanggal
				th No Transaksi
				th Pelanggan
				th Total Harga
			tbody: each model in filteredModel
				tr-transaction-status status=model.transaction_status
					td: format-datetime model.created_at
					td = model.code
					td = model.customer.name
					td: format-currency model.total_prices

					td
						bs-button clicked='showDetailTransaction' clickedParamBinding='model' size='xs' icon='glyphicon glyphicon-list' type='inverse' title='Detail'
						'
						bs-button clicked='destroyRecord' clickedParamBinding='model' size='xs' icon='glyphicon glyphicon-remove' type='danger' title='Delete'

	.col-sm-4 == outlet
